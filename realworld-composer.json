{
    "name": "realworld",
    "description": "Fullstack example codebases for Laravel 5.7.",
    "keywords": [ "framework", "laravel", "realworld" ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/gothinkster/realworld",
    "support": {
        "issues": "https://github.com/gothinkster/realworld/issues",
        "source": "https://github.com/simbo1905/laravel-realworld-example-app"
    },
    "authors": [
        {
            "name": "Sandeesh"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-cors": "^0.11.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "~1.0",
        "tymon/jwt-auth": "^1.0",
        "barryvdh/laravel-ide-helper": "^2.3",
        "fzaninotto/faker": "~1.4"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "your-cmd": [
            "cd /opt/app-root/src",
            "mkdir -p storage",
            "cp -rp /opt/app-root/src/public/storage/* /opt/app-root/src/storage",
            "rm -rf /opt/app-root/src/public/storage"
        ],
        "post-install-cmd": [
            "@your-cmd"
        ],
        "post-update-cmd": [
            "@your-cmd"
        ]
    }
}
